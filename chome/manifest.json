{
  "manifest_version": 2,
  "name": "Chrome Keyboard Hotkeys",
  "version": "1.3.2",
  "description": "Extension for customizing chrome keyboard hotkeys.",
  "icons": {
    "128": "icons/icon-128x128.png",
    "48":  "icons/icon-48x48.png",
    "16":  "icons/icon-16x16.png"
  },
   "commands": {
      "close_tab": {
         "description": "Close Tab [Current]"
      },
      "extensions_tab": {
        "description": "Extensions"
      },
      "chrome_apps": {
        "description": "Applications"
      }
   },
  "author": "Naacal",
  "background": {
    "scripts": [ "src/hotkeys.js" ]
  }
}